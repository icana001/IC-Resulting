{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nuxt",
  "buildCommand": "pnpm build",
  "installCommand": "pnpm install",
  "outputDirectory": ".output",
  "regions": ["fra1"],
  "env": {
    "NUXT_PUBLIC_TURNSTILE_SITE_KEY": "@turnstile_site_key"
  },
  "headers": [
    {
      "source": "/admin/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex, nofollow"
        }
      ]
    }
  ]
}
